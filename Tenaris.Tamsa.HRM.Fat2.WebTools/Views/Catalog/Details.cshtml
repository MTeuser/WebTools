@{
    ViewBag.Title = "Details";
}

<div>
    Detalles   
 <div id="Content">
     <table>
          
         @foreach (var item in ViewBag.Type)
         {
             <tr>
                 <div>@item.idType</div>
                 <div>@item.Code</div>
                 <div>@item.Name</div>
                 <div>@item.Description</div>
             </tr>
         }
     </table>
     <div id="cProperties" class="contentproperties">
         <table>
             @foreach (var p in ViewBag.Properties)
             {
                 <tr>
                     <td>@p.idProperty</td>
                     <td >@p.Name</td>                     
                     <td >@p.TypeName</td>                     
                     <td>@p.ViewUI</td>
                 </tr>
             }
         </table>
     </div>
 </div>
            
</div>

